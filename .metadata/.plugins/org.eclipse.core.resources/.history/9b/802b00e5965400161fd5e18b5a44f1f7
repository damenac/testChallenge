package root.aux;

public class CountedNumber {

	/** Number referenced in the position of the array */
	private Integer number;
	
	/** Amounf of duplicated values */
	private int count;
	
	public CountedNumber(Integer number, int count){
		this.number = number;
		this.count = count;
	}

	public Integer getNumber() {
		return number;
	}

	public void setNumber(Integer number) {
		this.number = number;
	}

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}
}
